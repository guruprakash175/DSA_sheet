class Solution {
    public boolean isIsomorphic(String s, String t) {
        
       int[] lastS = new int[256];
       int[] lastT = new int[256];

       for(int i=0; i<s.length();i++){
        char ch = s.charAt(i);
        char ch1 = t.charAt(i);

        if(lastS[ch] != lastT[ch1]){
            return false;
        }
        lastS[ch] = i + 1;
        lastT[ch1] = i + 1;
       }
       return true;
    }
}
